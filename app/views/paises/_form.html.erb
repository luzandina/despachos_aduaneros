<% form_for(@paise) do |f| %>
  <%= f.error_messages %>

<table width="40%" align="center"  >
  <tr>
    <td>
      <p>
       <%= f.label :nombre %><br />
       <%= f.text_field :nombre %>
     </p>
     
     <p>
       <%= f.label :codigo %><br />
       <%= f.text_field :codigo %>
     </p>
     
    </td>
  </tr>
</table>
<table width="40%" align="center"  >
  <tr>
    <td>
      <p>
       <%= f.submit 'Salvar' %>
     </p>
    </td>
  </tr>
</table>  
<% end %>


