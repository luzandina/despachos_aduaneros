<h1>Usuarios</h1>

<%= link_to 'Nuevo Usuario', new_usuario_path %>

<table border="1">
  <tr>
    <th>Nombres</th>
    <th>Paterno</th>
    <th>Materno</th>
    <th>Email</th>
    </tr>

<% @usuarios.each do |usuario| %>
  <tr>
    <td><%=h usuario.nombres %></td>
    <td><%=h usuario.paterno %></td>
    <td><%=h usuario.materno %></td>
    <td><%=h usuario.email %></td>
    <td><%= link_to 'Ver', usuario %></td>
    <td><%= link_to 'Editar', edit_usuario_path(usuario) %></td>
    <td><%= link_to 'Borrar', usuario, :confirm => 'Estas seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />


